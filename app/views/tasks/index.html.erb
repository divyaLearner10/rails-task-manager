<div class="container">
  <div class="page-header">
    <h1>My tasks</h1>
  </div>
  <hr class="border border-danger border-2 opacity-50">
  <% @tasks.each do |task| %>
      <p><%= link_to task.title , task_path(task)%></p>
  <% end %>
  <%= link_to "Add new task", new_task_path, class: "btn btn-secondary"%>
</div>
